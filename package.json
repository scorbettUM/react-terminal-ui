{
  "name": "@shapes-org/react-terminal-ui",
  "version": "1.0.0",
  "description": "A terminal react component with support for light and dark modes.",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "types": "build/index.d.ts",
  "files": [
    "build/index.*"
  ],
  "scripts": {
    "build": "./bin/build.sh",
    "install-peers": "install-peers -f",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scorbettUM/react-terminal-ui.git"
  },
  "keywords": [
    "terminal",
    "react",
    "component",
    "light",
    "dark"
  ],
  "author": "Sean Corbett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scorbettUM/react-terminal-ui/issues"
  },
  "homepage": "https://github.com/scorbettUM/react-terminal-ui#readme",
  "dependencies": {},
  "devDependencies": {
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^27.0.2",
    "@types/react": "^16.14.15",
    "@types/react-dom": "^16.9.14",
    "identity-obj-proxy": "^3.0.0",
    "install-peers-cli": "^2.2.0",
    "jest": "^27.2.1",
    "ts-jest": "^27.1.4",
    "typescript": "^3.9.10"
  },
  "peerDependencies": {
    "react": ">16.13.0",
    "react-dom": ">16.13.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "coverageDirectory": "docs/coverage"
  }
}
